{
    "manifest_version": 2,

    "name": "Clocker",
    "description": "Automated Testing of Browser Performance",
    "version": "0.0.1",
    "background": {
        "scripts": [
            "background/rxjs.umd.min.js",
            "background/background.js",
            "background/job.js",
            "background/page.js",
            "background/iteration.js",
            "background/rawData.js",
            "background/dataCollection.js",
            "background/arrayFunctions.js"
        ],
        "persistent": true
    },
    "browser_action": {
        "default_title": "Automated Testing of Browser Performance"
    },
    "icons": {
        "16": "logo16.png",
        "32": "logo32.png",
        "48": "logo48.png",
        "128": "logo128.png"
    },
    "content_security_policy": "script-src 'self' 'sha256-ELkDE2InNI702G86lymxXYBdrcJq5bMn8+PhTgN6cks='; object-src 'self'",
    "permissions": [
        "unlimitedStorage",
        "webRequest",
        "webNavigation",
        "tabs",
        "debugger",
        "<all_urls>"
    ]
}
